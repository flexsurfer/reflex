{
  "name": "@flexsurfer/reflex",
  "version": "0.1.20",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flexsurfer/reflex.git"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest --passWithNoTests",
    "test:clean": "jest --passWithNoTests --silent",
    "test:watch": "jest --passWithNoTests --watch",
    "test:ci": "jest --passWithNoTests --coverage --silent",
    "prepublishOnly": "npm run test:ci && npm run build"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.0.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.2",
    "ts-jest": "^29.4.0",
    "tsup": "^7.2.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "immer": "^10.1.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "keywords": [
    "re-frame",
    "state-management",
    "reactive",
    "event-driven",
    "subscriptions",
    "effects",
    "functional",
    "typescript",
    "react",
    "react-native",
    "unidirectional-data-flow",
    "frontend",
    "frp",
    "store",
    "pubsub",
    "handler"
  ]
}
